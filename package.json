{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec babel-node ./server/bin/www",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "yarn run clean && yarn run build-babel",
    "heroku-postbuild": "cd frontend && yarn install && yarn run build",
    "start": "node /build/server/bin/www"
  },
  "engines": {
    "node": "12.8.0",
    "yarn": "1.17.3",
    "npm": "6.10.3"
  },
  "dependencies": {
    "aws-sdk": "^2.756.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-fileupload": "^1.2.0",
    "http-proxy-middleware": "^1.0.5",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "morgan": "~1.9.1",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "npm-run-all": "^4.1.5",
    "pg": "^8.3.3",
    "uuid": "^8.3.0",
    "nodemon": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "make-runnable": "^1.3.8"
  }
}
